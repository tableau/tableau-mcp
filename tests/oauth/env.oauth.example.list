# Copy to .env.oauth and fill in values for local OAuth tests.
# Generate a private key: openssl genrsa -out private.pem
# Then use OAUTH_JWE_PRIVATE_KEY_PATH=./private.pem or OAUTH_JWE_PRIVATE_KEY="$(cat private.pem)"
OAUTH_ISSUER=http://127.0.0.1:3927
OAUTH_JWE_PRIVATE_KEY_PATH=
OAUTH_CLIENT_ID_SECRET_PAIRS=test-client-id:test-client-secret
OAUTH_DISABLE_SCOPES=false
TABLEAU_MCP_TEST=true
SERVER=
SITE_NAME=
